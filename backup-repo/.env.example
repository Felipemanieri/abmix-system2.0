# Configurações do Banco de Dados PostgreSQL
DATABASE_URL=postgresql://user:password@host:port/database

# Configurações do Make.com (Webhooks)
# URLs de webhook geradas no Make.com para notificações em tempo real
MAKE_WEBHOOK_PROPOSALS=https://hook.make.com/seu-webhook-propostas
MAKE_WEBHOOK_FINANCIAL=https://hook.make.com/seu-webhook-financeiro  
MAKE_WEBHOOK_SHEETS=https://hook.make.com/seu-webhook-sheets

# APIs do Google (para integrações diretas se necessário)
GOOGLE_DRIVE_API_KEY=sua-chave-google-drive
GOOGLE_SHEETS_API_KEY=sua-chave-google-sheets
GOOGLE_CLIENT_ID=seu-google-client-id
GOOGLE_CLIENT_SECRET=seu-google-client-secret

# Configurações de desenvolvimento
NODE_ENV=development

# Configurações de sessão
SESSION_SECRET=seu-secret-super-seguro-aqui

# URLs de webhook adicionais para integrações específicas
WEBHOOK_COMERCIAL=https://hook.make.com/comercial-webhook
WEBHOOK_DOCUMENTOS=https://hook.make.com/documentos-webhook
WEBHOOK_NOTIFICACOES=https://hook.make.com/notificacoes-webhook